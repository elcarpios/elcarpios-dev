---
import ShellockLogo from "@assets/svg/shellock.svg";
import Project from "@components/Project.astro";

export const prerender = true;
---

<Project
  title="Shellock App"
  ,
  subtitle="Shellock Platform and API"
  description="Real-time updates from anywhere in the world managed with some clicks"
  ,
  image={{
    src: ShellockLogo,
    alt: "Shellock logo",
    class: "bg-gray-50",
  }}
